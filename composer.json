  "description": "A role base access control",
  "license": "MIT",
  "keywords": [
    "sk-rbac",
    "role-base-access-control",
    "role base access control",
    "role manager",
    "premission manager"
  ],
  "minimum-stability": "stable",
  "version": "1.2",
